/*Square pattern 1 - i/p: 3       o/p: 111
                                       222
                                       333*/
#include<stdio.h>
void main()
{
    int n,i,j;
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n;j++)
        {
          printf("%d",i);
        }
        printf("\n");
    }
}
