/*Triangle pattern2  i/p : 4  o/p : 1
                                    22
                                    333
                                    4444 */                                                                                                                    
#include<stdio.h>
void main()
{
    
    int n,i,j;
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=i;j++)
        {
            printf("%d",i);
        }
        printf("\n");
    }
}
