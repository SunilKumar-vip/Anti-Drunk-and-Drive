/* Program to find even numbers in the given array    
i/p : 5             o/p : 4 2
1 3 4 2 5           
     
     
     
                                                     */
#include<stdio.h>
void main()
{
    int n,i,a[100];
    scanf("%d",&n);
    for(i=1;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    
    for(i=1;i<n;i++)
    {
        if(a[i]%2==0)
        {
            printf("%d ",a[i]);
        }
    }
}
