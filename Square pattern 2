/* Square pattern 2    i/p : 4   o/p: 1234 
                                      1234
                                      1234
                                      1234  */

#include<stdio.h>
void main()
{
    int n,i,j;
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=n;j++)
        {
            printf("%d",j);
        }
        printf("\n");
    }
}
