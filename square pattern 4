/* Square pattern 4  i/p: 4  o/p : 4321
                                   4321
                                   4321
                                   4321      */

#include<stdio.h>
void main()
{
    int n,i,j;
    scanf("%d",&n);
    for(i=n;i>0;i--)
    {
        for(j=n;j>0;j--)
        {
            printf("%d",j);
        }
        printf("\n");
    }
}
