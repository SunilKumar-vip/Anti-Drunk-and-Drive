/* Triangle pattern 1 i/p : 4  o/p : 1
                                     12
                                     123
                                     1234 */
#include<stdio.h>
void main()
{
    int n,i,j;
    scanf("%d",&n);
    for(i=1;i<=n;i++)
    {
        for(j=1;j<=i;j++)
        {
            printf("%d",j);
        }
        printf("\n");
    }
}
